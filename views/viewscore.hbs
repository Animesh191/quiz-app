<div class="dashcontainer z-depth-4">

    <nav>
        <div class="nav-wrapper dashnav">
            <a href="/" class="brand-logo right">Quiz Application</a>
            <ul id="nav-mobile" class="left hide-on-med-and-down">
                <li><a href="/logout">Logout</a></li>
                <li><a href="/dashboard">Dashboard</a></li>
                <li><a href="/create/quiz">Create Quiz</a></li>
            </ul>
        </div>
    </nav>



    <div class="row">
        <div class="col s12">
            <div class="card #0097a7 cyan darken-2">
                <div class="card-content white-text">
                    <span class="card-title">{{{quiz_name}}}</span>
                    <p>You scored {{{score}}}% in the quiz</p>
                </div>
                <div class="card-action">
                    <a href="{{{quiz_link}}}">Attempt again</a>
                </div>
            </div>
        </div>
    </div>


</div>